{
  "name": "javascript",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "bin": {
    "javascript": "npm-cli.js"
  },
  "directories": {
    "doc": "doc",
    "lib": "lib"
  },
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "watch": "webpack --watch",
    "build": "webpack",
    "lup:start": "npm run lup:serve -- --open=chrome",
    "lup:watch": "webpack --watch",
    "lup:build": "cross-env NODE_ENV=production webpack --mode=production",
    "lup:coverage": "cross-env NODE_ENV=test nyc mocha",
    "lup:cucumber": "cross-env NIGHTWATCH_ENV=chrome cucumber-js tests/cucumber/features/**/*.feature --require @babel/register --require cucumber.conf.js --require tests/cucumber/step-definitions --format node_modules/cucumber-pretty --format json:.reports/cucumber/report.json",
    "lup:e2e": "nightwatch",
    "lup:format": "prettier --check {src,tests}/**",
    "lup:lint": "eslint {src,tests}/**/*.{html,js,json,jsx,ts,tsx,gql,graphql}",
    "lup:reinstall": "npm run lup:remove && npm install",
    "lup:remove": "rm -rf node_modules package-lock.json",
    "lup:serve": "cross-env NODE_ENV=development webpack serve --devtool=source-map",
    "lup:test": "cross-env NODE_ENV=test mocha",
    "lup:update": "ncu -t minor -u && npm install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^6.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.2",
    "html-webpack-plugin-remove": "^0.1.0",
    "image-webpack-loader": "^7.0.1",
    "url-loader": "^4.1.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.2",
    "@leanup/cli": "^1.0.101",
    "@leanup/cli-preact": "^1.0.101",
    "@leanup/git-hooks": "^1.0.100",
    "chromedriver": "88.0.0",
    "cssnano": "4.1.10",
    "geckodriver": "1.22.1",
    "npm-check-updates": "11.1.4",
    "selenium-server": "3.141.59",
    "tailwindcss": "2.0.3",
    "typescript": "4.1.5"
  },
  "dependencies": {
    "@leanup/cli": "^1.0.101",
    "@leanup/cli-preact": "^1.0.101",
    "css-loader": "^5.0.2",
    "fs": "0.0.1-security",
    "mini-css-extract-plugin": "^1.3.8",
    "style-loader": "^2.0.0",
    "@leanup/form": "^1.0.100",
    "@leanup/lib": "^1.0.100",
    "navigo": "8.8.12",
    "preact": "10.5.12",
    "rxjs": "6.6.3"
  },
  "node": {
    "fs": "empty"
  }
}
